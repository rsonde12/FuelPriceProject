<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Manage Profile</title>
  <link rel="stylesheet" href="global.css">
</head>
<header>
  <nav>
    <ul>
      <li><a href="/fuel">Fuel Form</a></li>
      <li><a href="/fuelhistory">Fuel History</a></li>
      <li><a class="active" href="/mangeProfile">View Profile</a></li>
      <li><a href="/logout">Sign Out</a></li>
    </ul>
  </nav>
</header><body>
  <div><h1>Client Profile</h1></div>
  <table class="table">
    <%if(clientProfileList.length > 0) {%>
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Address</th>
        <th scope="col">Address2</th>
        <th scope="col">City</th>
        <th scope="col">State</th>
        <th scope="col">Zipcode</th>
      </tr>
    </thead>
    <%}%>
    <tbody>
      <%if(clientProfileList.length > 0) {%>
        <%clientProfileList.forEach(item => {%>
            <tr>
              <td scope="row"><%= item.name %></td>
              <td><%= item.address %></td>
              <td><%= item.address2 %></td>
              <td><%= item.city %></td>
              <td><%= item.state %></td>
              <td><%= item.zipcode %></td>
            </tr>
        <%})%>
        <%} else{%>
        <p>No Client Profile available</p>
      <%}%>
    </tbody>
  </table>
</body>
</html>
