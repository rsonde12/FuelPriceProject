<%- include("layout/header") %>
<body>
  <div><h1>Fuel Quote History</h1></div>
  <table class="table">
    <%if(fuelList.length > 0) {%>
    <thead>
      <tr>
        <th scope="col">Gallons</th>
        <th scope="col">Price</th>
        <th scope="col">Total Price</th>
      </tr>
    </thead>
    <%}%>
    <tbody>
      <%if(fuelList.length > 0) {%>
        <%fuelList.forEach(item => {%>
          <%if(item.userid == "1") {%>
            <tr>
              <td scope="row"><%= item.gallons %></td>
              <td><%= item.price %></td>
              <td><%= item.total %></td>
            </tr>
          <%}%>
        <%})%>
      <%} else{%>
        <p>No fuel history available</p>
      <%}%>
    </tbody>
  </table>
</body>
</html>
